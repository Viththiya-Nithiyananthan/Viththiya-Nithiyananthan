<script>
<function addNum(a, b) {
  return a + b;
}
{
document.getElementById("addNum").innerHTML = addNum(10, 2);
}

function multiNum(a, b) {
  return a * b;
}
{
document.getElementById("multiNum").innerHTML = multiNum(10, 2);
}


function subtractNum(a, b) {
  return a - b;
}
{
document.getElementById("subtractNum").innerHTML = subtractNum(10, 2);
}

function divideNum(a, b) {
  return a / b;
}
{
document.getElementById("divideNum").innerHTML =divideNum(10, 2);
}
</script>
